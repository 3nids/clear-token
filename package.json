{
  "name": "docbot",
  "version": "1.0.7",
  "description": "Create an issue in remote repo when a PR is labeled.",
  "files": [
    "action.yml",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "git@github.com:opengisch/qgis-docbot.git",
  "author": "Denis Rouzaud <denis.rouzaud@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.2",
    "@actions/github": "^2.0.0",
    "@octokit/webhooks": "^7.0.0",
    "@types/node": "^13.1.2",
    "@types/promise-retry": "^1.1.3",
    "@zeit/ncc": "^0.20.5",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build": "ncc build src/index.ts --minify --v8-cache",
    "check-prettier": "yarn run prettier --check",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 \"./**/*.{js,ts}\"",
    "format-prettier": "yarn run prettier --write",
    "prettier": "prettier --ignore-path .gitignore \"./**/*.{js,json,md,ts,yml}\""
  }
}
